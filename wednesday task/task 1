public class Main { // Main class
    public static void main(String[] args) { // Thread 1
        System.out.println("From Main thread!");
        System.out.println("From Main thread!");
        System.out.println("From Main thread!");

        MainThread1 t1 = new MainThread1();
        t1.start(); // Calls run method - Thread 2

        MainThread2 t2 = new MainThread2();
        Thread tt2 = new Thread(t2);
        new Thread(t2).start(); // Thread 3
        tt2.start(); // Thread 4

        // Creating and starting a separate thread for MainThreadOne
        MainThreadOne tOne = new MainThreadOne();
        tOne.start(); // Thread 5

        // Exception handling for division by zero
        try {
            System.out.println(10 / 0); // This will cause an exception
        } catch (ArithmeticException e) {
            System.out.println("Error: Division by zero is not allowed in the main thread.");
        }
    }
}

// MainThread1 extends Thread class
class MainThread1 extends Thread {
    public void run() {
        System.out.println("From MainThread1! " + this.getName());
    }
}

// MainThread2 implements Runnable interface
class MainThread2 implements Runnable {
    public void run() {
        // Handling division by zero in MainThread2
        try {
            System.out.println(10 / 0);
        } catch (ArithmeticException e) {
            System.out.println("Error in MainThread2: Division by zero is not allowed.");
        }
    }
}

// New MainThreadOne class extending Thread
class MainThreadOne extends Thread {
    public void run() {
        System.out.println("From MainThreadOne!");
        System.out.println("From MainThreadOne!");
        System.out.println("From MainThreadOne!");
    }
}
